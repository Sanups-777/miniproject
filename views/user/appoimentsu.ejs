<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>
  <body>
    <table class="table">
      <thead>
        <tr>
          <th scope="col">Businessname</th>
          <th scope="col">Username</th>
          <th scope="col">Date</th>
          <th scope="col">Service</th>
        </tr>
      </thead>
      <tbody>
        <% appointmentlist.forEach(Appointments => { %> <% if(Appointments.uname
        === username) { %>
        <tr>
          <td><%= Appointments.bname %></td>
          <td><%= Appointments.uname %></td>
          <td><%= Appointments.date %></td>
          <td><%= Appointments.service %></td>
          <td><%= Appointments.issue %></td>
        </tr>
        <% } %> <% }) %>
      </tbody>
    </table>
  </body>
</html>
